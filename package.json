{
  "name": "nestjs.prisma.setup",
  "version": "0.0.0",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "dotenv -e .env.dev -- nest build",
    "build:prod": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" \"prisma/**/*.ts\" \"config**/*.ts\" \"./mod.ts\"",
    "start": "nest start",
    "dev": "dotenv -e .env.dev -- nest start --watch",
    "start:dev": "dotenv -e .env.dev -- nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "dotenv -e .env.testing -- jest",
    "test:watch": "dotenv -e .env.testing -- jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "dotenv -e .env.testing -- jest --no-cache --config ./test/jest-e2e.json",
    "db:migrate": "npx prisma migrate dev ",
    "db:seed": "npx prisma db seed ",
    "db:migrate:dev": "dotenv -e .env.dev -- npx prisma migrate dev ",
    "db:seed:dev": "dotenv -e .env.dev -- npx prisma db seed ",
    "db:migrate:testing": "dotenv -e .env.testing -- npx prisma migrate dev ",
    "db:seed:testing": "dotenv -e .env.testing -- npx prisma db seed ",
    "tinker": "dotenv -e .env.dev -- ts-node ./tinker.ts "
  },
  "dependencies": {
    "@nestjs-modules/mailer": "^1.8.1",
    "@nestjs/common": "^7.6.17",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^7.6.17",
    "@nestjs/jwt": "^9.0.0",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^7.6.17",
    "@nestjs/platform-socket.io": "^9.1.2",
    "@nestjs/swagger": "^4.8.0",
    "@nestjs/websockets": "^9.1.2",
    "@prisma/client": "^4.4.0",
    "@types/passport-jwt": "^3.0.7",
    "bcryptjs": "^2.4.3",
    "class-transformer": "0.4.0",
    "class-validator": "^0.13.1",
    "compression": "^1.7.4",
    "dotenv": "^16.0.3",
    "express": "^4.18.1",
    "graphql": "^16.6.0",
    "handlebars": "^4.7.7",
    "helmet": "^6.0.0",
    "html-escaper": "^3.0.3",
    "lodash": "^4.17.21",
    "nodemailer": "^6.8.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.6",
    "swagger-ui-express": "^4.1.6"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.6.0",
    "@nestjs/schematics": "^7.3.1",
    "@nestjs/testing": "7.6.18",
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.22",
    "@types/node": "^18.8.0",
    "@types/nodemailer": "^6.4.6",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^6.0.0",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "prisma": "^4.4.0",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.4",
    "ts-loader": "^8.0.18",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.8.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}
